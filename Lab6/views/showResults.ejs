<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
        <h2>Appealing (1-5 Scale) </h2>
        <ul>
        <% for (var i=0; i<results[0].length; i++){ %>
            <li><%= results[0][i].q1 %> :  <%= results[0][i].count %></li>
        <% } %>    
        </ul>


        <h2>Efficient</h2>
        <ul>
        <% for (var i=0; i<results[1].length; i++){ %>
            <li><%= results[1][i].q2 %> :  <%= results[1][i].count %></li>
        <% } %>    
        </ul>

        <h2>How To Apply</h2>
        <ul>
        <% for (var i=0; i<results[2].length; i++){ %>
            <li><%= results[2][i].q3 %> :  <%= results[2][i].count %></li>
        <% } %>    
        </ul>

        <h2>One Word</h2>
        <ul>
        <% for (var i=0; i<results[3].length; i++){ %>
            <li><%= results[3][i].q4 %> :  <%= results[3][i].count %></li>
        <% } %>    
        </ul>

        <h2>Crowded</h2>
        <ul>
        <% for (var i=0; i<results[4].length; i++){ %>
            <li><%= results[4][i].q5 %> :  <%= results[4][i].count %></li>
        <% } %>    
        </ul>

        <h2>Comfortable (On a 1-5 Scale)</h2>
        <ul>
        <% for (var i=0; i<results[5].length; i++){ %>
            <li><%= results[5][i].q6 %> :  <%= results[5][i].count %></li>
        <% } %>    
        </ul>
             
</body>
</html>